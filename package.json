{
   "name": "sis001-downloader",
   "version": "0.0.1",
   "engines": {
      "node": ">= 12.22.7",
      "vscode": "^1.22.0"
   },
   "description": "",
   "main": "dist/extension/extension.js",
   "exports": {
      ".": "./dist/cli.js",
      "./extension": "./dist/extension/extension.js"
   },
   "categories": [
      "Other"
   ],
   "activationEvents": [
      "onCommand:video-previewer.parseNewList"
   ],
   "contributes": {
      "commands": [
         {
            "command": "video-previewer.parseNewList",
            "title": "解析新作品列表"
         }
      ],
      "configuration": [
         {"title": "数据存储位置", "properties": {
            "sis001-downloader.database": {
               "type": "string",
               "description": "存放数据库的位置"
            },
            "sis001-downloader.logger": {
               "type": "string",
               "description": "存放日志的位置"
            }
         }}
      ]
   },
   "scripts": {
      "test": "jest",
      "prebuild": "yarn lint",
      "build": "tsc",
      "watch": "tsc -watch",
      "debug": "ts-node src/cli.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "lint": "eslint --fix src --ext .ts",
      "cli": "yarn build && node --trace-warnings dist/cli.js",
      "migration": "typeorm migration:run"
   },
   "husky": {
      "hooks": {
         "pre-commit": "yarn build"
      }
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/AmatsuZero/TrySelenium.git"
   },
   "author": "amatsuzero",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/AmatsuZero/TrySelenium/issues"
   },
   "homepage": "https://github.com/AmatsuZero/TrySelenium#readme",
   "dependencies": {
      "@notionhq/client": "^0.4.4",
      "dotenv": "^10.0.0",
      "reflect-metadata": "^0.1.10",
      "selenium-webdriver": "^4.1.0",
      "sqlite3": "^5.0.2",
      "typeorm": "0.2.41"
   },
   "devDependencies": {
      "@babel/core": "^7.16.0",
      "@babel/eslint-parser": "^7.16.3",
      "@babel/preset-env": "^7.16.4",
      "@babel/preset-typescript": "^7.16.0",
      "@types/jest": "^27.0.3",
      "@types/node": "^16.11.11",
      "@types/selenium-webdriver": "^4.0.16",
      "@types/vscode": "^1.63.0",
      "@typescript-eslint/eslint-plugin": "^5.5.0",
      "@typescript-eslint/parser": "^5.5.0",
      "@vscode/test-electron": "^2.0.0",
      "babel-jest": "^27.4.2",
      "eslint": "^8.3.0",
      "eslint-config-alloy": "^4.4.0",
      "eslint-plugin-jest": "^25.3.0",
      "husky": "^7.0.4",
      "jest": "^27.4.3",
      "ts-node": "^10.4.0",
      "typescript": "^4.5.2"
   }
}
