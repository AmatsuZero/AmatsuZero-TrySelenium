{
   "type": "sqlite",
   "database": "data/database.sqlite",
   "synchronize": true,
   "logging": true,
   "entities": [
      "dist/entity/**/*.js"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "dist/subscriber/**/*.js"
   ],
   "migrationsTableName": "info_model",
   "cli": {
      "entitiesDir": "dist/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "dist/subscriber"
   }
}